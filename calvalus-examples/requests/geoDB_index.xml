<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <ows:Identifier>GeoDB</ows:Identifier>

    <wps:DataInputs>
        <wps:Input>
            <ows:Identifier>productionName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>example GeoDB create index</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>geoInventory</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/geoInventory/example</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>inputPath</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/eodata/MER_RR__1P/r03/${yyyy}/${MM}/${dd}/MER_..._1.*${yyyy}${MM}${dd}.*.N1</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        

        <wps:Input>
            <ows:Identifier>minDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-03</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>maxDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-05</wps:LiteralData>
            </wps:Data>
        </wps:Input>

    </wps:DataInputs>
</wps:Execute>