<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>
    <property>
        <name>fs.defaultFS</name>
	    <value>hdfs://calvalus</value>
    </property>
    <property>
        <name>dfs.nameservices</name>
        <value>calvalus</value>
    </property>
    <property>
        <name>dfs.ha.namenodes.calvalus</name>
        <value>nn1,nn2</value>
    </property>
    <property>
        <name>dfs.namenode.rpc-address.calvalus.nn1</name>
        <value>master00:8020</value>
    </property>
    <property>
        <name>dfs.namenode.rpc-address.calvalus.nn2</name>
        <value>master01:8020</value>
    </property>
    <property>
        <name>dfs.namenode.http-address.calvalus.nn1</name>
        <value>master00:50070</value>
    </property>
    <property>
        <name>dfs.namenode.http-address.calvalus.nn2</name>
      <value>master01:50070</value>
      </property>
    <property>
        <name>dfs.ha.automatic-failover.enabled</name>
        <value>true</value>
    </property>
    <property>
      <name>dfs.client.failover.proxy.provider.calvalus</name>
      <value>org.apache.hadoop.hdfs.server.namenode.ha.ConfiguredFailoverProxyProvider</value>
    </property>
</configuration>
