<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:map='urn:import:com.bc.calvalus.portal.client.map'
             xmlns:date='urn:import:com.google.gwt.user.datepicker.client'
        >

  <g:HTMLPanel>
      <g:RadioButton ui:field='dateSelDateList' name="dateSel"/>
      <g:RadioButton ui:field='dateSelDateRange'  name="dateSel"/>
      <date:DateBox ui:field='minDate'/>
      <date:DateBox ui:field='maxDate'/>
      <g:TextBox ui:field='numDays'/>
      <g:TextArea ui:field='dateList'/>
      <map:RegionMapWidget ui:field='predefinedRegions'/>

      <g:HTMLPanel>
      </g:HTMLPanel>
  </g:HTMLPanel>

</ui:UiBinder>